                form(action="/")
                    .row.form_calc
                        .col-xl-3.col-lg-3.col-md-3
                            label(for="destination").text-left Выдача:
                            select(name="destination" id="destination")
                                option В Забайкальске
                                option В Новосибирске
                                option В Москве
                                option В Нижнем Новгороде
                        .col-xl-3.col-lg-3.col-md-3
                            label(for="type_of_product").text-left Вид товара:
                            select(name="type_of_product" id="type_of_product")
                                option Запчасти
                                option Игрушки
                                option Одежда
                                option Оборудование
                        .col-xl-3.col-lg-3.col-md-3
                            label(for="city").text-left Город отправки:
                            select(name="city" id="city")
                                option Гуанчжоу
                                option Иу
                                option Гонконг
                        .col-xl-3.col-lg-3.col-md-3
                            label(for="shipping_method").text-left Вариант отправки:
                            select(name="shipping_method" id="shipping_method")
                                option Авиа 7-10 дней
                                option ЖД 15-20 дней
                                option Морем 30-40 дней
                    .row
                        .col-xl-12.col-lg-12.col-md-12.text-center
                            h5.descr_data Ввод данных
                    .row
                        .col-xl-3.col-lg-3.col-md-3.form_data
                            span Габариты груза (см): 
                        .col-xl-3.col-lg-3.col-md-3.input_data
                            input(type="number", name="data_cargo" id="width_cargo").input_dimensions
                            span x
                            input(type="number", name="data_cargo" id="height_cargo").input_dimensions
                            span x
                            input(type="number", name="data_cargo" id="length_cargo").input_dimensions
                        .col-xl-3.col-lg-3.col-md-3.input_data_2
                            span Вес: 
                            input(type="number", name="weight_cargo" id="weight_cargo")
                        .col-xl-3.col-lg-3.col-md-3.input_data_2
                            span Объем: 
                            input(type="number", name="volume_cargo" id="volume_cargo")
                    .row.submint_calc
                        .col-xl-12.col-lg-12.col-md-12
                            button.btn.btn-danger Рассчитать



                        

            


